{
  "name": "Tab Scheduler",
  "author": "Julia",
  "version": "1.0",
  "manifest_version": 2,
  "description": "In theory, should automatically open tabs at a certain time of day in your timezone",
  "permissions": ["management"],
  "browser_action": {
    "default_popup": "window.html",
    "default_icon": "icon.png",
    "default_title": "Tab Scheduler"
  },

  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}